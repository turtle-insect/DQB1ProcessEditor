<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>disable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>DQB1ProcessEditor.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="DQB1ProcessEditor.ico" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Memory">
      <HintPath>external\Memory.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
